{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"noEmit": true,
		"declaration": false,
		"emitDeclarationOnly": false,
		"declarationMap": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false
	},
	"include": [
		"./**/*.ts",
		"./**/*.tsx",
		"../src/**/*.ts",
		"../src/**/*.tsx"
	]
}
