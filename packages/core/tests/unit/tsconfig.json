{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "..",
		"paths": {
			"mutts": [
				"../../../../../mutts/src/index.ts"
			],
			"mutts/*": [
				"../../../../../mutts/src/*"
			],
			"@pounce/core/jsx-runtime": [
				"src/runtime/jsx-runtime.ts"
			],
			"@pounce/core/jsx-dev-runtime": [
				"src/runtime/jsx-dev-runtime.ts"
			],
			"@pounce/core": [
				"src/lib/node/index.ts",
			]
		}
	},
	"include": [
		"../../src/**/*",
		"../**/*.ts",
		"../**/*.tsx"
	]
}
