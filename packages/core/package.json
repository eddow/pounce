{
  "name": "@pounce/core",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run build:plugin",
    "build:plugin": "esbuild src/babel-plugin-jsx-reactive.ts --bundle --format=esm --platform=node --outfile=dist/plugin.js",
    "build:all": "npm run build",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:ui": "playwright test --ui",
    "biome": "biome check --write src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddow/@pounce/core.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/node.js",
      "default": "./dist/dom.js"
    },
    "./dom": {
      "types": "./dist/index.d.ts",
      "import": "./dist/dom.js",
      "default": "./dist/dom.js"
    },
    "./node": {
      "types": "./dist/index.d.ts",
      "import": "./dist/node.js",
      "default": "./dist/node.js"
    },
    "./plugin": {
      "types": "./src/babel-plugin-jsx-reactive.ts",
      "import": "./dist/plugin.js",
      "default": "./dist/plugin.js"
    },
    "./runtime": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./runtime/jsx-runtime": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./runtime/jsx-dev-runtime": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/index.d.ts",
      "import": "./dist/node.js",
      "default": "./dist/node.js"
    },
    "./src": "./src/index.ts",
    "./src/*": "./src/*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@biomejs/biome": "^2.2.7",
    "@playwright/test": "^1.56.1",
    "@types/babel__core": "^7.20.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-legacy": "^7.2.1",
    "baseline-browser-mapping": "^2.9.11",
    "esbuild": "^0.24.0",
    "sass": "^1.93.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-babel": "^1.3.2",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@babel/types": "^7.28.6",
    "dockview-core": "^4.13.1",
    "jsdom": "^27.4.0",
    "mutts": "link:../../../mutts"
  },
  "packageManager": "pnpm@10.28.2"
}